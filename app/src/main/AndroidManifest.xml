<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="code.anushka.cal.kotlincalculatorapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/calculator"
        android:label="Calculator"
        android:roundIcon="@drawable/calculator"
        android:supportsRtl="true"
        android:theme="@style/Theme.KotlinCalculatorApp">
        <activity android:name="code.anushka.cal.kotlincalculatorapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>